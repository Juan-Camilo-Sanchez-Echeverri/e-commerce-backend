{
  "name": "store",
  "version": "1.0.0",
  "description": "",
  "author": "Juan Camilo Sanchez Echeverri",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "set NODE_ENV=local && nest start --watch",
    "start:debug": "set NODE_ENV=local && nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:docker": "docker compose up --build -d",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pm2:start:dev": "set NODE_ENV=development && npm i && npm run build && pm2 start --name store dist/main.js",
    "pm2:restart:dev": "set NODE_ENV=development && npm i && npm run build && pm2 restart --name store dist/main.js --update-env",
    "pm2:start:prod": "npm i && npm run build && set NODE_ENV=production && pm2 start --name store dist/main.js",
    "pm2:restart:prod": "npm i && npm run build && set NODE_ENV=production && pm2 restart --name store dist/main.js --update-env"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.750.0",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/event-emitter": "3.0.0",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "11.0.1",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/schedule": "5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.8.0",
    "dayjs": "1.11.13",
    "fast-xml-parser": "4.5.2",
    "joi": "^17.13.3",
    "mongoose": "^8.10.1",
    "mongoose-autopopulate": "1.1.0",
    "mongoose-paginate-v2": "1.9.0",
    "nodemailer": "^6.10.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "winston": "3.17.0",
    "winston-slack-webhook-transport": "2.3.6",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "11.0.4",
    "@nestjs/schematics": "11.0.1",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "5.0.0",
    "@types/multer": "1.4.12",
    "@types/node": "22.13.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "source-map-support": "0.5.21",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.20.0"
  }
